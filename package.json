  "scripts": {
    "dev-api": "set NEXT_PUBLIC_OPERATION_MODE=api&& vercel dev",
    "dev-ui": "set NEXT_PUBLIC_OPERATION_MODE=ui&& vercel dev",
    "dev-ui-noapi": "set NEXT_PUBLIC_OPERATION_MODE=ui&& set NEXT_PUBLIC_DISABLE_SELF_API=true&& vercel dev",
